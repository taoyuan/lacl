{
	"name": "AclUser",
	"plural": "aclusers",
	"base": "PersistedModel",
	"properties": {
		"id": {
			"type": "String",
			"id": true,
			"generated": false
		}
	},
	"relations": {
		"user": {
			"type": "belongsTo",
			"model": "User",
			"foreignKey": "userId"
		},
		"roles": {
			"type": "hasMany",
			"model": "AclRole",
			"through": "AclMapping",
			"foreignKey": "userId",
			"keyThrough": "roleId"
		},
		"permissions": {
			"type": "hasMany",
			"model": "AclPermission",
			"polymorphic": "subject"
		}
	}
}
